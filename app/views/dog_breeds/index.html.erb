<div class= "search">
<%= form_tag(dog_breeds_path, method: :get) do %>
  Dog Breed <%= text_field_tag :name, params[:name] %>
  <%= submit_tag 'Search', name: nil %>
<% end %>
</div>


<ul>
<% @dogs.each do |dog| %>
<li>
<p> <%= link_to "#{dog.name}", dog_breed_path(dog) %> </p> <br>
<%= image_tag("#{dog.image}", class: "dog_image", alt: "Image of Dog") %>
</li>
<% end %>
</ul>
