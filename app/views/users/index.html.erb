<h1> Hellooooo <%= @user.email %> </h1>
<% if @user.atts_table_id != nil %>
    <% @atts_table = AttsTable.find(@user.atts_table_id) %>
    <%= link_to "Your Preferences", atts_table_path(@atts_table)%>
<% else %>
    <%= link_to "Enter Your Preferences Here", new_atts_table_path %>
<% end %>

<br>

<% matches = @user.match_accesser %>

<% matches.each do |match| %>
<% dog = DogBreed.find(match) %>
<p> Name: <%= "#{dog.name}" %> </p> <br>
<p> Origins: <%= "#{dog.origins}" %> </p> <br>
<p> Life Span: <%= "#{dog.life_span}" %> </p> <br>
<p> Group: <%= "#{dog.group}" %> </p> <br>
<% end %>
